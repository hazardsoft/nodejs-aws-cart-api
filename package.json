{
  "name": "rs-checkout-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "prisma:generate": "prisma generate --schema ./prisma",
    "prisma:deploy": "prisma migrate deploy --schema ./prisma",
    "prisma:deploy:local": "env-cmd --file .env.local npx prisma migrate deploy --schema ./prisma",
    "prisma:deploy:remote": "env-cmd --file .env.remote npx prisma migrate deploy --schema ./prisma",
    "prisma:seed": "prisma db seed",
    "prisma:seed:local": "env-cmd --file .env.local prisma db seed",
    "prisma:seed:remote": "env-cmd --file .env.remote prisma db seed",
    "docker:up": "docker compose --env-file .env.local up --detach",
    "docker:down": "docker compose down",
    "docker:build": "docker build --tag hazardsoft/cart-service:latest .",
    "docker:push": "docker push hazardsoft/cart-service:latest",
    "eb:init": "eb init hazardsoft-cart-api",
    "eb:create": "env-cmd -x -f .env.remote eb create prod --cname hazardsoft-cart-api-prod --single --envvars DATABASE_URL=\\$DATABASE_URL",
    "eb:deploy": "eb deploy prod",
    "eb:destroy": "eb terminate --all"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.14.1",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.3",
    "@prisma/client": "^5.17.0",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^7.0.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@types/aws-lambda": "^8.10.141",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "copy-webpack-plugin": "^12.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^5.17.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "uuid": "^10.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
